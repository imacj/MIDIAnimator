{
    "nodes": [
        {
            "id": "get_midi_file",
            "name": "Get MIDI File",
            "description": "Gets a MIDI provided a path to the file with the file picker.",
            "executor": "rust",
            "realtime": true,
            "handles": {
                "inputs": [
                    {
                        "id": "file_path",
                        "name": "File Path",
                        "type": "String"
                    }
                ],
                "outputs": [
                    {
                        "id": "tracks",
                        "name": "Tracks",
                        "type": "Array<MIDITrack>"
                    },
                    {
                        "id": "stats",
                        "name": "MIDI Stats",
                        "type": "String"
                    }
                ]
            }
        },

        {
            "id": "get_midi_track_data",
            "name": "Get MIDI Track Data",
            "description": "Gets a MIDI track's data from an array of MIDI Tracks.",
            "executor": "rust",
            "realtime": true,
            "handles": {
                "inputs": [
                    {
                        "id": "tracks",
                        "name": "Tracks",
                        "type": "Array<MIDITrack>"
                    },
                    {
                        "id": "track_name",
                        "name": "Track Name",
                        "type": "String"
                    }
                ],
                "outputs": [
                    {
                        "id": "notes",
                        "name": "Notes",
                        "type": "Array<MIDINote>"
                    },
                    {
                        "id": "control_change",
                        "name": "Control Change",
                        "type": "HashMap<u8, Array<MIDIEvent>>"
                    },
                    {
                        "id": "pitchwheel",
                        "name": "Pitchwheel",
                        "type": "Array<MIDIEvent>"
                    },
                    {
                        "id": "aftertouch",
                        "name": "Aftertouch",
                        "type": "Array<MIDIEvent>"
                    }
                ]
            }
        },

        {
            "id": "keyframes_from_object",
            "name": "Keyframes from Object",
            "description": "Get keyframes from an object in the scene.",
            "executor": "rust",
            "realtime": true,
            "handles": {
                "inputs": [
                    {
                        "id": "object_groups",
                        "name": "ObjectGroups",
                        "type": "Array<ObjectGroup>"
                    },
                    {
                        "id": "object_group_name",
                        "name": "Object Group Name",
                        "type": "String"
                    },
                    {
                        "id": "object_name",
                        "name": "Object Name",
                        "type": "String"
                    }
                ],
                "outputs": [
                    {
                        "id": "dyn_output",
                        "name": "Dynamic Output",
                        "type": "Dyn<Array<Keyframe>>"
                    }
                ]
            }
        },

        {
            "id": "viewer",
            "name": "Viewer",
            "description": "Display node results",
            "executor": "rust",
            "realtime": true,
            "handles": {
                "inputs": [
                    {
                        "id": "data",
                        "name": "Input",
                        "type": "Any"
                    }
                ],
                "outputs": []
            }
        },

        {
            "id": "scene_link",
            "name": "Scene Link",
            "description": "Provide access to the scene",
            "executor": "rust",
            "realtime": true,
            "handles": {
                "inputs": [],
                "outputs": [
                    {
                        "id": "name",
                        "name": "Name",
                        "type": "String"
                    },
                    {
                        "id": "object_groups",
                        "name": "Object Groups",
                        "type": "Array<ObjectGroup>"
                    }
                ]
            }
        }
    ]
}
